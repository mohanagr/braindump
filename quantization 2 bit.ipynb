   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "b41d8f7d",
   "metadata": {},
   "outputs": [],
   "source": [
    "rho = np.linspace(1e-6,0.9999,1000)\n",
    "N=1e4\n",
    "Rinf=rho*np.sqrt(N)/np.sqrt(1+rho**2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "64d4a3d6",
   "metadata": {},
   "outputs": [],
   "source": [
    "rho2=2*np.arcsin(rho)/np.pi\n",
    "sigma2=np.sqrt(1/N - rho2**2/N)\n",
    "R2=rho2/sigma2"
   ]
  },
