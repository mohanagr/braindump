   "source": [
    "N=32\n",
    "x = np.zeros(N)\n",
    "y = x.copy()\n",
    "x[:2]=0.1\n",
    "x[-1:]=0.1\n",
    "y[:11]=1\n",
    "y[-11:]=1\n",
    "plt.plot(np.fft.fftshift(x))\n",
    "plt.plot(np.fft.fftshift(y))\n",
    "conv = np.fft.irfft(np.fft.rfft(x)*np.fft.rfft(y))\n",
    "plt.plot(np.fft.fftshift(conv))"
   ]
